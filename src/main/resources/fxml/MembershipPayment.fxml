<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mas.lms.controller.MembershipPaymentController">
    <Label text="Process Membership Payment" styleClass="title" />
    <TextField fx:id="memberIdField" promptText="Member ID" />
    <Button text="Search Member" onAction="#searchMember" />
    <TextField fx:id="amountField" promptText="Amount" />
    <Button text="Process Payment" onAction="#processPayment" />
</VBox>
