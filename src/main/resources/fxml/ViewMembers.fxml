<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="mas.lms.controller.ViewMembersController">
    <TableView fx:id="membersTable" prefHeight="400.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="idColumn" text="ID"/>
            <TableColumn fx:id="nameColumn" text="Name"/>
            <TableColumn fx:id="birthdateColumn" text="Birthdate"/>
            <TableColumn fx:id="memberIdColumn" text="Member ID"/>
            <TableColumn fx:id="goldMemberColumn" text="Gold Member"/>
            <TableColumn fx:id="daysBorrowedColumn" text="Days Borrowed"/>
        </columns>
    </TableView>
    <Button fx:id="updateMemberButton" text="Update Member"/>
    <Button fx:id="deleteMemberButton" text="Delete Member"/>
</VBox>
